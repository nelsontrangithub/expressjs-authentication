<div>
    <div class="page-header">
        <h1 class="text-info">{{title}}</h1>
    </div>
    <h3>Are you sure you want to delete the following student?</h3>
    <table class="table">
        <tr>
            <td>First Name: </td>
            <td>{{jsonData.FirstName}}</td>
        </tr>
        <tr>
            <td>Last Name: </td>
            <td>{{jsonData.LastName}}</td>
        </tr>
        <tr>
            <td>School: </td>
            <td>{{jsonData.School}}</td>
        </tr>
        <tr>
            <td>Started: </td>
            <td>{{jsonData.StartDate}}</td>
        </tr>
    </table>
    <form action="/delete" method="post" >
        <input type="hidden" name="_id" value="{{jsonData._id}}" /><br />
        <input type="submit" value="Delete" class="btn btn-primary"/>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="/" class="btn btn-primary">Cancel</a>
    </form>
</div>
